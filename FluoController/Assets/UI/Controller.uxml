    <engine:VisualElement name="controller" style="position: absolute; top: 40px; bottom: 40px; left: 100px; right: 100px; justify-content: space-evenly;">
        <engine:VisualElement name="row" style="flex-direction: row;">
            <engine:VisualElement name="section" class="control-section">
                <engine:Label text="Camera" class="control-section-label" />
