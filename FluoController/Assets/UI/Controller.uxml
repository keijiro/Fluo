                <VJUITK.VJKnob name="knob-9" label="Palette" high-value="1" />
                <VJUITK.VJButton name="button-0" label="Shuffle" />
                <VJUITK.VJKnob name="knob-3" label="Sustain" high-value="1" value="0.5" />
